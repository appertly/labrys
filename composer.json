{
    "name": "appertly/labrys",
    "type": "library",
    "description": "A library that helps you write web applications in Hack",
    "keywords": ["hack", "web", "webapp", "web application"],
    "homepage": "https://github.com/appertly/labrys",
    "license": "Apache-2.0",
    "extra": {
        "branch-alias": {
            "dev-master": "0.6.x-dev"
        }
    },
    "require": {
        "hhvm": ">=3.12.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.0",
        "caridea/acl": "^2.1.2",
        "caridea/auth": "^2.0.0",
        "caridea/container": "^2.2.0",
        "caridea/dao": "^2.0.0",
        "caridea/http": "^2.0.0",
        "caridea/validate": "^2.0.0",
        "aura/router": "^3.0",
        "appertly/ducts": "~0.1",
        "appertly/axe": "~0.1"
    },
    "require-dev": {
        "hackpack/hackunit": "^1.0",
        "mockery/mockery": "^0.9",
        "hack-psr/psr7-http-message-hhi": "^0.2",
        "libreworks/psr3-log-hhi": "^1.0.0",
        "libreworks/ext-mongodb-hhi": "^1.2.0",
        "mongodb/mongodb": "^1.1",
        "zendframework/zend-diactoros": "^1.0"
    },
    "suggest": {
        "ext-mongodb": "Allows use of Labrys\\Db MongoDB classes",
        "mongodb/mongodb": "Version 1.1.x allows use of Labrys\\Db\\MongoFileService"
    },
    "autoload": {
        "classmap": [
            "xhp"
        ],
        "psr-4": {
            "Labrys\\": "src/"
        },
        "files": [
            "src/typedefs.hh"
        ]
    }
}
